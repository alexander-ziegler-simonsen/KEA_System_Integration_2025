{
  "name": "auto-doc-gen",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config ./config_files/vite.config.ts",
    "build": "tsc -b && vite build --config ./config_files/vite.config.ts",
    "lint": "eslint . --config ./config_files/eslint.config.js",
    "preview": "vite preview --config ./config_files/vite.config.ts",
    "doc": "npx typedoc --entryPointStrategy Expand src"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typedoc": "^0.28.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
